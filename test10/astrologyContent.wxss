.body {
  display: flex;
  flex-direction: column;
  justify-content: space-between;

}

.bg {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  z-index: -1;
}

.topView {
  width: 100%;
  height: 260rpx;
  display: flex;
  align-items: flex-start;
  justify-content: center;
  flex-direction: column;
}

.toptext {
  font-size: 20rpx;
  line-height: 32rpx;
  color: white;
  margin-left: 186rpx;
}

.topper {
  font-size: 32rpx;
  line-height: 32rpx;
  color: white;
  margin-left: 186rpx;
}

.toptitle {
  font-size: 60rpx;
  line-height: 60rpx;
  color: white;
  margin-left: 70rpx;
  padding-top: 52rpx;
}

/* bottom */

.listbg {
  width: auto;
  height: auto;
  background-color: white;
  margin: 0 30rpx 30rpx 30rpx;
  border-radius: 24rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  box-sizing: border-box;
  padding: 10rpx 30rpx;
}

.celltitle {
  font-size: 32rpx;
  line-height: 44rpx;
  color: #666666;
  padding: 80rpx 0 40rpx 0;
  display: block;
}

.cellcontont {
  font-size: 28rpx;
  line-height: 1.5em;
  color: #666666;
}

/* 进度球 */
.avatar-box {
  width: 100rpx;
  height: 100rpx;
  background: #f00;
  overflow:hidden;
  border-radius: 50rpx;
  margin-left: 70rpx;
}
.avatar-box .avatar{
  width: 100rpx;
  height: 100rpx;
}


.water-group{
  position: relative;
  height: 100rpx;
  width: 100rpx;
  border-radius: 50rpx;
  overflow: hidden;
}
.water-group .water{
  position: absolute;
  width: 1000rpx;
  height: 100rpx;
  display: block;
  background-size: 50% 100%;
  background-image: url("data:image/png;base64,");
}


.water-group .water1{
  top: 100rpx;
  left: -900rpx;
  opacity: 0.2;
  animation: water-right 20s infinite linear;
}
.water-group .water2{
  top:102rpx;
  left: 0;
  opacity: 0.3;
  animation: water-left 30s infinite linear;
}
.water-group .water3{
  top: 104rpx;
  left: -900rpx;
  opacity: 0.5;
  animation: water-right 40s infinite linear;
}

