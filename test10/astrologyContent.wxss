@keyframes water-right{
  0% {
    transform: translateX(0) translateZ(0) scaleY(1)
  }
  50% {
    transform: translateX(450rpx) translateZ(0) scaleY(0.85)
  }
  100% {
    transform: translateX(900rpx) translateZ(0) scaleY(1)
  }
}
@keyframes water-left{
  0% {
    transform: translateX(0) translateZ(0) scaleY(1)
  }
  100% {
    transform: translateX(-900rpx) translateZ(0) scaleY(1)
  }
}